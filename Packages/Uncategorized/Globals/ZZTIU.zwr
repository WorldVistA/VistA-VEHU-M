^ZZTIU(8925,11460,"TEXT",1,0)="Patient: A 52-year-old male OEF/OIF veteran who is a 100% Service connected, "
^ZZTIU(8925,11460,"TEXT",2,0)="whose testing was ordered by his internist."
^ZZTIU(8925,11460,"TEXT",3,0)=""
^ZZTIU(8925,11460,"TEXT",4,0)="Chief Complaint: Gradual hearing loss bilaterally after being exposed to a road "
^ZZTIU(8925,11460,"TEXT",5,0)="side IED blast.  He reports that his tinnitus is annoying but not life altering."
^ZZTIU(8925,11460,"TEXT",6,0)=""
^ZZTIU(8925,11460,"TEXT",7,0)="Problem Focused Case History:"
^ZZTIU(8925,11460,"TEXT",8,0)="* He has never had his hearing tested previously and he has never worn"
^ZZTIU(8925,11460,"TEXT",9,0)="amplification"
^ZZTIU(8925,11460,"TEXT",10,0)="* History of head injury"
^ZZTIU(8925,11460,"TEXT",11,0)="* History of noise exposure, tinnitus."
^ZZTIU(8925,11460,"TEXT",12,0)="* No history of cerumen impaction, dizziness, ear drainage or ear pain. "
^ZZTIU(8925,11460,"TEXT",13,0)="* Medication list includes Aspirin, Seroquel and Multivitamins.  Patient does "
^ZZTIU(8925,11460,"TEXT",14,0)="not currently take any other prescription medication, over the counter "
^ZZTIU(8925,11460,"TEXT",15,0)="medications or multivitamins"
^ZZTIU(8925,11460,"TEXT",16,0)=""
^ZZTIU(8925,11460,"TEXT",17,0)="Otoscopy unremarkable bilaterally."
^ZZTIU(8925,11460,"TEXT",18,0)=""
^ZZTIU(8925,11460,"TEXT",19,0)="Procedures Performed:"
^ZZTIU(8925,11460,"TEXT",20,0)="* Comprehensive hearing test (92557)"
^ZZTIU(8925,11460,"TEXT",21,0)="* Tympanometry and acoustic reflex threshold testing (92550)"
^ZZTIU(8925,11460,"TEXT",22,0)="* Assessment of tinnitus (92625)"
^ZZTIU(8925,11460,"TEXT",23,0)="* No depression screen was performed"
^ZZTIU(8925,11460,"TEXT",24,0)=""
^ZZTIU(8925,11460,"TEXT",25,0)="Diagnosis:"
^ZZTIU(8925,11460,"TEXT",26,0)="* Sensorineural hearing loss, bilateral (389.18)"
^ZZTIU(8925,11460,"TEXT",27,0)="* Mixed hearing loss, unilateral (389.21)"
^ZZTIU(8925,11460,"TEXT",28,0)="* Otorrhea, unspecified (388.60)"
^ZZTIU(8925,11460,"TEXT",29,0)=""
^ZZTIU(8925,11460,"TEXT",30,0)="Recommendations:"
^ZZTIU(8925,11460,"TEXT",31,0)="* Referral to otolaryngologist for an otologic evaluation and medical clearance "
^ZZTIU(8925,11460,"TEXT",32,0)="due to tinnitus and asymmetry between ears"
^ZZTIU(8925,11460,"TEXT",33,0)="* Evaluation for binaural amplification pending medical evaluation and clearance"
