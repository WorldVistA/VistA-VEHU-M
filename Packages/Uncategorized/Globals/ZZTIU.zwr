^ZZTIU(8925,11521,"TEXT",2,0)="REASON FOR VISIT/CHIEF COMPLAINT:"
^ZZTIU(8925,11521,"TEXT",3,0)="  Evaluation and management of acute problem(s)"
^ZZTIU(8925,11521,"TEXT",4,0)=""
^ZZTIU(8925,11521,"TEXT",5,0)="  Patient presents to Urgent Care clinic today c/o uticaria since 12/16/2014."
^ZZTIU(8925,11521,"TEXT",6,0)=""
^ZZTIU(8925,11521,"TEXT",7,0)=""
^ZZTIU(8925,11521,"TEXT",8,0)="INTERVAL HISTORY:"
^ZZTIU(8925,11521,"TEXT",9,0)=""
^ZZTIU(8925,11521,"TEXT",10,0)="  Patient denies having this problem before.  Rash started 1 day after PCP "
^ZZTIU(8925,11521,"TEXT",11,0)="  visit.  No new foods, environmental allergens.  Suspect this reaction is "
^ZZTIU(8925,11521,"TEXT",12,0)="  to metformin which was started on 12/15."
^ZZTIU(8925,11521,"TEXT",13,0)=""
^ZZTIU(8925,11521,"TEXT",14,0)="PHYSICAL EXAMINATION: "
^ZZTIU(8925,11521,"TEXT",15,0)="  Uticaria is present on back, shoulders and chest.  Worse today than yesterday."
^ZZTIU(8925,11521,"TEXT",16,0)=""
^ZZTIU(8925,11521,"TEXT",17,0)="INTERVAL LABS:"
^ZZTIU(8925,11521,"TEXT",18,0)="  n/a"
^ZZTIU(8925,11521,"TEXT",19,0)=""
^ZZTIU(8925,11521,"TEXT",20,0)="ASSESSMENT & PLAN: "
^ZZTIU(8925,11521,"TEXT",21,0)="  d/c metformin"
^ZZTIU(8925,11521,"TEXT",22,0)="  enter new allergy"
^ZZTIU(8925,11521,"TEXT",23,0)="  follow up with PCP next available appointment"
