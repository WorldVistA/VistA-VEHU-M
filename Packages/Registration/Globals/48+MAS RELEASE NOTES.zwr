^DG(48,4.1,"NO",23,"C",2,0)=" "
^DG(48,4.1,"NO",23,"C",3,0)="This option is a menu containing the options used by the Beneficiary"
^DG(48,4.1,"NO",23,"C",4,0)="Travel Clerk/Supervisor."
