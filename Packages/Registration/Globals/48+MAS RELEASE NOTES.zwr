^DG(48,4.7,"C",8,0)="A.  Sample Installation Times"
^DG(48,4.7,"C",9,0)=" "
^DG(48,4.7,"C",10,0)="The following are installation times of MAS version 4.7 at our"
^DG(48,4.7,"C",11,0)="test sites:"
^DG(48,4.7,"C",12,0)="                         PDP          VAX         ISM"
^DG(48,4.7,"C",13,0)="--------------------------------------------------------"
