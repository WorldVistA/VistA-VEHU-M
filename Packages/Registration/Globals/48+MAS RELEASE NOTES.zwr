^DG(48,5,"CO",30,"C",66,0)="     ------  ---------------------  ----- ---- ----- ----  ----- ----"
^DG(48,5,"CO",30,"C",67,0)="      3      Admit Psych 4/1"
^DG(48,5,"CO",30,"C",68,0)="             Transfer AA 4/5"
